/reducers/categories.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/reducers/products.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/products/index.js",["86"],"import React, { useEffect, useState } from 'react'\nimport { connect } from 'react-redux'\nimport Wireframes from '../../common/components/Wireframes'\nimport getProducts from '../../store/actions/getProducts'\nimport Table from './table'\nimport Pagination from '../../common/components/Pagination'\nimport Filters from './filters'\nimport \"../../common/css/products.css\"\n\nconst Products = (props) => {\n    const { data } = props.products;\n    //set pagination\n    const [currentPage, setCurrentPage] = useState(1);\n    const dataPerPage = 10;\n    const indexOfLastData = currentPage * dataPerPage\n    const indexOfFirstData = indexOfLastData - dataPerPage\n    const currentData = data.slice(indexOfFirstData, indexOfLastData)\n\n    //change page\n    function paginate(pageNumber) {\n        setCurrentPage(pageNumber)\n        //Persistence pageNumber state\n        sessionStorage.setItem('pageNumber',pageNumber)\n    }\n\n    let { isLoading } = props.products;\n    function getProductsData() {\n        return props.dispatch(getProducts())\n    }\n\n    useEffect(() => {\n        //check pageNumber, if exist go that page\n        let pageN = sessionStorage.getItem('pageNumber')\n        if(pageN){\n            paginate(pageN)\n        }\n        getProductsData()\n    }, [])\n\n    return (\n        <Wireframes>\n            <Filters {...props} />\n            {isLoading ? (\n                <div className=\"loading-icon\" >\n                    <div className=\"spinner-border \" role=\"status\">\n                        <span className=\"sr-only\">Loading...</span>\n                    </div>\n                </div>\n            ) : (\n                    <div className=\"table\">\n                        <Table {...props} data={currentData} />\n                        <Pagination\n                            {...props}\n                            dataPerPage={dataPerPage}\n                            totalData={data.length}\n                            paginate={paginate} />\n                    </div>\n                )}\n\n        </Wireframes>\n    )\n}\n\nexport default connect(res => ({ ...res }))(Products) ","/Users/junnanshi/Desktop/ab_react_project/src/common/components/Wireframes.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/common/components/Footer.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/common/components/Header.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/getProducts.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/http.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/getCategories.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/products/table.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/details/index.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/common/components/Pagination.js",["87","88"],"import React, { useEffect, useState } from 'react';\nimport '../css/pagination.css'\n\nconst Pagination = (props) => {\n    const { dataPerPage, totalData, paginate } = props\n    const pageNumbers = []\n    const [activeIndex, setActiveIndex] = useState(1)\n    const totalPage = Math.ceil(totalData / dataPerPage)\n    for (let i = 1; i <= totalPage; i++) {\n        pageNumbers.push(i)\n    }\n\n    useEffect(() => {\n        const pageN = sessionStorage.getItem(\"pageNumber\")\n        if (pageN) {\n            setActiveIndex(pageN)\n        }\n    }, [])\n\n    return (\n        <nav>\n            <ul className=\"pagination\">\n                {pageNumbers.map(number => (\n                    <li key={number} className={`${(number == activeIndex ? \"page-active\" : \"\")} page-item`} onClick={() => {\n                        setActiveIndex(number)\n                        paginate(number)\n                    }}>\n                        <a href=\"#\" className=\"page-link\">\n                            {number}\n                        </a>\n                    </li>\n                ))}\n            </ul>\n        </nav>\n    );\n}\n\nexport default Pagination;","/Users/junnanshi/Desktop/ab_react_project/src/view/products/filters.js",["89","90"],"import React, { useEffect } from 'react';\nimport getCategories from '../../store/actions/getCategories';\nimport getProducts from '../../store/actions/getProducts';\nimport '../../common/css/filters.css'\n\nconst Filters = (props) => {\n    const { data } = props.products\n    const { isLoading } = props.categories\n    const { categories } = props.categories\n    const getCategoriesData = (cateArr) => {\n        return props.dispatch(getCategories(cateArr))\n    }\n\n    useEffect(() => {\n        //get all categories in current data\n        let cateArr = []\n        data.map(item => {\n            if (item.categories.length > 0) {\n                cateArr = cateArr.concat(item.categories)\n            }\n        })\n        //Deduplication\n        cateArr = Array.from(new Set(cateArr))\n        getCategoriesData(cateArr)\n    }, [])\n\n    const reloadProduct = (itemId) => {\n        return props.dispatch(getProducts(itemId))\n    }\n\n    return (\n        <div className=\"categories-block\">\n            {isLoading ? (\n                <span></span>\n            ) : (   //show first 5 categories using to filte\n                    categories.slice(0, 6).map(item => (\n                        <span className=\"categories-order\" onClick={() => {\n                            //request from server for exact catefory products\n                            reloadProduct(item.id)\n                            //setPageNumber = 1\n                            sessionStorage.setItem(\"pageNumber\",1)\n                        }\n                        } key={item.id}>{item.name}</span>\n                    ))\n                )}\n        </div>\n    );\n}\n\nexport default Filters;",{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":4,"column":1,"nodeType":"97","endLine":7,"endColumn":2},{"ruleId":"98","severity":1,"message":"99","line":38,"column":8,"nodeType":"100","endLine":38,"endColumn":10,"suggestions":"101"},{"ruleId":"102","severity":1,"message":"103","line":24,"column":60,"nodeType":"104","messageId":"105","endLine":24,"endColumn":62},{"ruleId":"106","severity":1,"message":"107","line":28,"column":25,"nodeType":"108","endLine":28,"endColumn":59},{"ruleId":"109","severity":1,"message":"110","line":17,"column":23,"nodeType":"111","messageId":"112","endLine":17,"endColumn":25},{"ruleId":"98","severity":1,"message":"113","line":25,"column":8,"nodeType":"100","endLine":25,"endColumn":10,"suggestions":"114"},"no-native-reassign",["115"],"no-negated-in-lhs",["116"],"import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getProductsData'. Either include it or remove the dependency array.","ArrayExpression",["117"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","React Hook useEffect has missing dependencies: 'data' and 'getCategoriesData'. Either include them or remove the dependency array.",["118"],"no-global-assign","no-unsafe-ne