nanshi/Desktop/ab_react_project/src/store/reducers/categories.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/reducers/products.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/products/index.js",["86"],"import React, { useEffect, useState } from 'react'\nimport { connect } from 'react-redux'\nimport Wireframes from '../../common/components/Wireframes'\nimport getProducts from '../../store/actions/getProducts'\nimport Table from './table'\nimport Pagination from '../../common/components/Pagination'\nimport Filters from './filters'\nimport \"../../common/css/products.css\"\n\nconst Products = (props) => {\n    const { data } = props.products;\n    const [currentPage, setCurrentPage] = useState(1);\n    const dataPerPage = 10;\n    //set pagination\n    const indexOfLastData = currentPage * dataPerPage\n    const indexOfFirstData = indexOfLastData - dataPerPage\n    const currentData = data.slice(indexOfFirstData, indexOfLastData)\n\n    //change page\n    function paginate(pageNumber) {\n        setCurrentPage(pageNumber)\n        //Persistence pageNumber state\n        localStorage.setItem('pageNumber',pageNumber)\n    }\n\n    let { isLoading } = props.products;\n    function getProductsData() {\n        return props.dispatch(getProducts())\n    }\n\n    useEffect(() => {\n        //check pageNumber, if exist go that page\n        let pageN = localStorage.getItem('pageNumber')\n        if(pageN){\n            paginate(pageN)\n        }\n        getProductsData()\n    }, [])\n\n    return (\n        <Wireframes>\n            <Filters {...props} />\n            {isLoading ? (\n                <div className=\"loading-icon\" >\n                    <div className=\"spinner-border \" role=\"status\">\n                        <span className=\"sr-only\">Loading...</span>\n                    </div>\n                </div>\n            ) : (\n                    <div className=\"table\">\n                        <Table {...props} data={currentData} />\n                        <Pagination\n                            {...props}\n                            dataPerPage={dataPerPage}\n                            totalData={data.length}\n                            paginate={paginate} />\n                    </div>\n                )}\n\n        </Wireframes>\n    )\n}\n\nexport default connect(res => ({ ...res }))(Products) ","/Users/junnanshi/Desktop/ab_react_project/src/common/components/Wireframes.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/common/components/Footer.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/common/components/Header.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/getProducts.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/http.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/store/actions/getCategories.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/products/table.js",[],"/Users/junnanshi/Desktop/ab_react_project/src/view/details/index.js",["87"],"import React, { useEffect } from 'react'\nimport { connect } from 'react-redux'\nimport Wireframes from \"../../common/components/Wireframes\"\nimport '../../common/css/details.css'\n\nconst Details = (props) => {\n    const { data } = props.location\n    const { categories } = props.categories\n    const { history } = props\n    console.log(props)\n\n    useEffect(() => {\n        //had problems here...\n        //the api /products?categories:in=... looks not return the correct results..\n        //may be I was wrong\n        const findeCateNames = () => {\n            let names = []\n            for (let item of data.categories) {\n                for (let cateName of categories) {\n                    if (item === cateName.id) {\n                        names.push(cateName.name)\n                    }\n                }\n            }\n            return names\n        }\n    })\n\n    const goBack = (history) => {\n        return () => {\n            if (history.length > 1) {\n                history.goBack();\n            } else {\n                history.push(\"/\")\n            }\n        }\n    }\n    return (\n        <Wireframes>\n            <div className=\"details\">\n                <div className=\"back-list\" onClick={goBack(history)}>&lt; Back to list</div>\n                <h1>{data.name}</h1>\n                <h5>Categories:\n                {data.categories.map((item, index) => (\n                    <span key={index}> {item}, </span>\n                ))}\n                </h5>\n                <h5>SKU:<span> {data.sku}</span></h5>\n                <h5>Price:<span> ${data.price.toFixed(2)}</span></h5>\n                <h5>Description:</h5><div dangerouslySetInnerHTML={{ __html: data.description }} />\n            </div>\n        </Wireframes >\n    )\n}\n\nexport default connect(res => res)(Details) ","/Users/junnanshi/Desktop/ab_react_project/src/common/components/Pagination.js",["88","89"],"/Users/junnanshi/Desktop/ab_react_project/src/view/products/filters.js",["90","91"],"import React, { useEffect } from 'react';\nimport getCategories from '../../store/actions/getCategories';\nimport getProducts from '../../store/actions/getProducts';\nimport '../../common/css/filters.css'\n\nconst Filters = (props) => {\n    const { data } = props.products\n    const { isLoading } = props.categories\n    const { categories } = props.categories\n    const getCategoriesData = (cateArr) => {\n        return props.dispatch(getCategories(cateArr))\n    }\n\n    useEffect(() => {\n        //get all categories in current data\n        let cateArr = []\n        data.map(item => {\n            if (item.categories.length > 0) {\n                cateArr = cateArr.concat(item.categories)\n            }\n        })\n        //Deduplication\n        cateArr = Array.from(new Set(cateArr))\n        getCategoriesData(cateArr)\n    }, [])\n\n    const reloadProduct = (itemId) => {\n        return props.dispatch(getProducts(itemId))\n    }\n\n    r