st 5 categories using to filte\n                    categories.slice(0, 6).map(item => (\n                        <span className=\"categories-order\" onClick={() => {\n                            //request from server for exact catefory products\n                            reloadProduct(item.id)\n                        }\n                        } key={item.id}>{item.name}</span>\n                    ))\n                )}\n        </div>\n    );\n}\n\nexport default Filters;",{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":4,"column":1,"nodeType":"100","endLine":7,"endColumn":2},{"ruleId":"101","severity":1,"message":"102","line":38,"column":8,"nodeType":"103","endLine":38,"endColumn":10,"suggestions":"104"},{"ruleId":"94","replacedBy":"105"},{"ruleId":"96","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":16,"column":15,"nodeType":"109","messageId":"110","endLine":16,"endColumn":29},{"ruleId":"111","severity":1,"message":"112","line":24,"column":60,"nodeType":"113","messageId":"114","endLine":24,"endColumn":62},{"ruleId":"115","severity":1,"message":"116","line":28,"column":25,"nodeType":"117","endLine":28,"endColumn":59},{"ruleId":"118","severity":1,"message":"119","line":17,"column":23,"nodeType":"120","messageId":"121","endLine":17,"endColumn":25},{"ruleId":"101","severity":1,"message":"122","line":25,"column":8,"nodeType":"103","endLine":25,"endColumn":10,"suggestions":"123"},"no-native-reassign",["124"],"no-negated-in-lhs",["125"],"import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getProductsData'. Either include it or remove the dependency array.","ArrayExpression",["126"],["124"],["125"],"no-unused-vars","'findeCateNames' is assigned a value but never used.","Identifier","unusedVar","eqeqeq","Expected '===' an